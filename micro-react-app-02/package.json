{
  "name": "react-app-02",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:development": "cross-env CUSTOM_ENV=development node scripts/start.js",
    "start:test": "cross-env CUSTOM_ENV=test node scripts/start.js",
    "start:production": "cross-env CUSTOM_ENV=production node scripts/start.js",
    "build:development": "pnpm eslint && cross-env CUSTOM_ENV=development node scripts/build.js",
    "build:test": "pnpm eslint && cross-env CUSTOM_ENV=test node scripts/build.js",
    "build:production": "pnpm eslint && cross-env CUSTOM_ENV=production node scripts/build.js",
    "test": "node scripts/test.js",
    "eslint": "cross-env NODE_ENV=production eslint --fix ./src",
    "preview": "serve -s -u ./build"
  },
  "engines": {
    "node": "^14.20.0 || ^16.13.0",
    "pnpm": ">=7.10.0"
  },
  "packageManager": "pnpm@7.10.0",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.195",
    "@types/node": "^18.16.18",
    "@types/react": "17.0.62",
    "@types/react-dom": "17.0.20",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.3.0",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "bfj": "7.0.2",
    "camelcase": "6.3.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "css-loader": "4.3.0",
    "dotenv": "8.6.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.10.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "3.10.2",
    "eslint-webpack-plugin": "2.7.0",
    "file-loader": "6.2.0",
    "fs-extra": "9.1.0",
    "html-webpack-plugin": "4.5.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-resolve": "26.6.2",
    "jest-watch-typeahead": "0.6.5",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "0.12.0",
    "modern-normalize": "1.1.0",
    "optimize-css-assets-webpack-plugin": "5.0.8",
    "pnp-webpack-plugin": "1.7.0",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.1",
    "postcss-safe-parser": "5.0.2",
    "prompts": "2.4.2",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dev-utils": "11.0.4",
    "react-dom": "17.0.2",
    "react-refresh": "0.14.0",
    "react-router-dom": "5.3.4",
    "resolve": "1.22.3",
    "resolve-url-loader": "3.1.5",
    "sass": "1.63.6",
    "sass-loader": "10.4.1",
    "semver": "7.5.3",
    "style-loader": "1.3.0",
    "terser-webpack-plugin": "4.2.3",
    "ts-pnp": "1.2.0",
    "typescript": "4.9.5",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.1",
    "web-vitals": "1.1.2",
    "webpack": "4.46.0",
    "webpack-dev-server": "3.11.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/qs": "6.9.7",
    "ahooks": "3.7.8",
    "antd": "5.6.3",
    "axios": "0.27.2",
    "babel-plugin-import": "1.13.6",
    "cross-env": "7.0.3",
    "less": "2.7.3",
    "less-loader": "5.0.0",
    "lint-staged": "12.5.0",
    "qs": "6.11.2",
    "serve": "13.0.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "./node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
