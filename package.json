{
  "name": "micro-web",
  "version": "1.0.0",
  "description": "基于qiankun及micro-app的微前端demo",
  "main": "",
  "scripts": {
    "start": "yarn i && yarn build && yarn preview",
    "i": "yarn && yarn clear-node-modules && yarn && node script/install.js ./",
    "build": "node script/build.js ./ && yarn copy-file",
    "copy-file": "node script/copy-file.js",
    "clear-node-modules": "node ./script/clearNodeModules.js",
    "preview": "node ./server.js",
    "pm2:start": "pm2 start pm2.config.js",
    "pm2:delete": "pm2 delete pm2.config.js",
    "pm2:restart": "pm2 restart pm2.config.js",
    "pm2:deploy": "yarn copy-file && yarn && yarn restart"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/8696/micro-web-demo.git"
  },
  "keywords": [],
