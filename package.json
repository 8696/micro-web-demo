  },
  "license": "ISC",
  "devDependencies": {
    "koa": "^2.13.4",
    "koa-static": "^5.0.0"
  },
  "dependencies": {
    "@types/koa": "^2.13.5",
    "fs-extra": "^10.1.0"
  }
}
